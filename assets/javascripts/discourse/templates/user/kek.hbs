<div class='shinbakaz2'>
	{{#if model.fbB}}
		<img src='https://i.imgur.com/ZcxqN8c.png'>
	{{else}}
		{{#if model.fbG}}
			<img src='https://i.imgur.com/rmydNkF.png'>
		{{else}}
			<img src='https://i.imgur.com/OMygNKW.png'>
		{{/if}}
	{{/if}}
	<br>
	<span style='color: #27ae60'>{{model.fbG}}</span> / <span style='color: gray'>{{model.fbN}}</span> / <span style='color: lightcoral'>{{model.fbB}}</span>
	<br>
	<button>Оставить отзыв</button>
	<hr>
	<table>
	{{#each model.FEEDBACKS as |feedback|}}
		<tr>
			<td><div class='malabela2 {{feedback.COLOR}}'>+{{feedback.SCORE}}</div></td>
			<td>{{feedback.FEEDBACK}}</td>
			<td><a href='/u/{{feedback.pNAME}}'>{{feedback.pNAME}}</a></td>
			<td>{{feedback.DATE}}</td>
		</tr>
	{{/each}}
	</table>

	<ul class="pagination">
	{{#each pages as |page|}}
		<li><button>{{page}}</button></li>
	{{/each}}
	</ul>
</div>
