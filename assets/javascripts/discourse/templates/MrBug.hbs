				<td>
					<button class="headin{{if bagoGuidaz " bi4"}}" {{action 'bagoGuidaz'}}>Инструкции</button>
				</td>
				<td>
					<button class="headin{{if bagoPlati " bi4"}}" {{action 'bagoPlati'}}>Оплата</button>
				</td>
			</tr>
			{{#if bagoPravila}}
